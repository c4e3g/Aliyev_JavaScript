function AliyevAQA() {
  var numberInput = prompt("What number did you enter?");
  var number = +numberInput; 

  if (number > 7) {
    console.log("Hello");
  }
  // Prompting so that a user can enter a number and running the function

  var name = prompt("What name did you enter:");
  if (name === "John") {
    console.log("Hello, John");
  } else {
    console.log("There is no such name");
  }
  // Prompting so that a user can enter a name

  var arrayInput = prompt("Enter an array of numbers, separated by spaces:");
  var array = arrayInput.split(' ').map(function(num) { return parseFloat(num); });

  // Output elements that are multiples of 3
  for (var i = 0; i < array.length; i++) {
    if (array[i] % 3 === 0) {
      console.log(array[i]);
    }
  }
}

// Aliyev AQA
AliyevAQA();
